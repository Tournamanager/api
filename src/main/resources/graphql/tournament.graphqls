type Tournament {
    id: ID!,
    name: String,
    description: String,
    owner: User,
    numberOfTeams: Int,
}

type Query {
    tournaments(count: Int):[Tournament]
    tournament(id: ID):Tournament
}

type Mutation {
    createTournament(id: ID, name:String!, description:String, owner: User, numberOfTeams: Int):Tournament
}